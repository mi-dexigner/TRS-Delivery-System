<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @if (@ViewBag.Title != null)
    {
        <title>@ViewBag.Title - TARS Delivery System </title>
    }
    else
    {
        <title>TARS Delivery System </title>
    }
    <link href="~/Content/Assets/Front/plugins/bootstrap-3.3.6/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Assets/Front/plugins/bootstrap-select-1.10.0/dist/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/Content/Assets/Front/plugins/font-awesome-4.6.1/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Assets/Front/plugins/owl.carousel.2/~/Content/Assets/Front//owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/Assets/Front/css/animate.css" rel="stylesheet" />
    <link href="~/Content/Assets/Front/css/theme.css" rel="stylesheet" />
    <link href="~/Content/Styles.css" rel="stylesheet" type="text/css" />

</head>
<body id="home">
    <!-- Main Wrapper -->
    <main class="wrapper">
        <!-- Header -->
        <header class="header-main header-style3">

            <!-- Header Topbar -->
            <div class="top-bar2">
                <div class="theme-container container">
                    <div class="row">
                        <div class="col-md-4 col-sm-12">
                            <a class="navbar-logo" href="@Url.Action("Index","Home")"> <img src="~/Content/Assets/Front//img/logo/logo-black.png" alt="logo" /> </a>
                        </div>
                        <div class="col-md-6 col-sm-12 fs-12 text-right">
                            <ul class="list-inline">
                                <li> <h6 class="font2-light"> Come to office </h6> <p class="theme-clr  font2-title1"> 24 Arham Street, NY, USA </p> </li>
                                <li> <h6 class="font2-light"> Want to meet? </h6> <p class="theme-clr  font2-title1"> Mon-Sat,9am-6pm </p> </li>
                                <li> <h6 class="font2-light"> Need a help? </h6> <p class="theme-clr font2-title1"> +880-1756-390-370 </p></li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.Header Topbar -->
            <!-- Header Logo & Navigation -->
            <nav class="menu-bar font2-title1 white-clr">
                <div class="theme-container container">
                    <div class="row">
                        <div class="col-xs-12 visible-xs">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-controls="navbar">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                           
                        </div>
                        <div class="col-md-10 col-sm-10 col-xs-12 fs-12">
                            <a class="sticky-logo hidden-sm" href="@Url.Action("Index","Home")"> <img alt="" src="~/Content/Assets/Front//img/logo/logo-white2.png" /> </a>
                            <div id="navbar" class="collapse navbar-collapse no-pad">
                                <ul class="navbar-nav theme-menu">
                                    <li class="@(ViewContext.RouteData.Values["Action"].ToString() == "Index" ? "active" : "")"> @Html.ActionLink("Home", "Index", "Home") </li>
                                    <li class="@(ViewContext.RouteData.Values["Action"].ToString() == "About" ? "active" : "")">  @Html.ActionLink("About", "About", "pages")</li>
                                    <li class="@(ViewContext.RouteData.Values["Action"].ToString() == "Tracking" ? "active" : "")">  @Html.ActionLink("Tracking", "Tracking", "pages")</li>
                                    <li class="@(ViewContext.RouteData.Values["Action"].ToString() == "Contact" ? "active" : "")">  @Html.ActionLink("Contact", "Contact", "pages")</li>
                                   
                                </ul>
                            </div>
                        </div>
                       
                    </div>
                </div>
            </nav>
            <!-- /.Header Logo & Navigation -->

        </header>
        <!-- /.Header -->
        @RenderBody()

        <!-- Footer -->
        <footer>
            <div class="footer-main pad-120 white-clr">
                <div class="theme-container container">
                    <div class="row">
                        <div class="col-md-3 col-sm-6 footer-widget">
                            <a href="#"> <img class="logo" alt="#" src="~/Content/Assets/Front//img/logo/logo-white.png" />  </a>
                        </div>
                        <div class="col-md-3 col-sm-6 footer-widget">
                            <h2 class="title-1 fw-900">quick links</h2>
                            <ul>
                                <li> @Html.ActionLink("Home", "Index", "Home") </li>
                                <li>@Html.ActionLink("About", "About", "pages") </li>
                                <li>@Html.ActionLink("Tracking", "Tracking", "pages") </li>
                                <li>@Html.ActionLink("Contact", "Contact", "pages") </li>
                            </ul>
                        </div>
                        <div class="col-md-3 col-sm-6 footer-widget">
                            <h2 class="title-1 fw-900">important links</h2>
                            <ul>
                                <li> <a href="#">Support</a> </li>
                                <li> <a href="#">Help</a> </li>
                                <li> <a href="#">Payment</a> </li>
                                <li> <a href="#">Privacy</a> </li>
                            </ul>
                        </div>
                        <div class="col-md-3 col-sm-6 footer-widget">
                            <h2 class="title-1 fw-900">get in touch</h2>
                            <ul class="social-icons list-inline">
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".20s"> <a href="#" class="fa fa-facebook"></a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".25s"> <a href="#" class="fa fa-twitter"></a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".30s"> <a href="#" class="fa fa-google-plus"></a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".35s"> <a href="#" class="fa fa-linkedin"></a> </li>
                            </ul>
                            <ul class="payment-icons list-inline">
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".20s"> <a href="#"> <img alt="#" src="~/Content/Assets/Front//img/icons/payment-1.png" /> </a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".25s"> <a href="#"> <img alt="#" src="~/Content/Assets/Front//img/icons/payment-2.png" /> </a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".30s"> <a href="#"> <img alt="#" src="~/Content/Assets/Front//img/icons/payment-3.png" /> </a> </li>
                                <li class="wow fadeIn" data-wow-offset="50" data-wow-delay=".35s"> <a href="#"> <img alt="#" src="~/Content/Assets/Front//img/icons/payment-4.png" /> </a> </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="footer-bottom">
                <div class="theme-container container">
                    <div class="row">
                        <div class="col-md-6 col-sm-6">
                            <p> &copy; Copyright @DateTime.Now.Year, All rights reserved </p>
                        </div>
                        <div class="col-md-6 col-sm-6 text-right">
                            <p> Design and <span class="theme-clr fa fa-heart"></span>  by <a href="#" class="main-clr">Muhammad Idrees &amp; Muhammad Ahmer </a> </p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
     
        <!-- /.Footer -->
    </main>
    <!-- / Main Wrapper -->
    <!-- Top -->
    <div class="to-top theme-clr-bg transition"> <i class="fa fa-angle-up"></i> </div>

    <script src="~/Content/Assets/Front/js/jquery-2.2.4.min.js"></script>
    <script src="~/Content/Assets/Front/plugins/bootstrap-3.3.6/js/bootstrap.min.js"></script>
    <script src="~/Content/Assets/Front/plugins/bootstrap-select-1.10.0/dist/js/bootstrap-select.min.js"></script>
    <script src="~/Content/Assets/Front/plugins/owl.carousel.2/owl.carousel.min.js"></script>
    <script src="~/Content/Assets/Front/js/jquery.sticky.js"></script>
    <script src="~/Content/Assets/Front/plugins/WOW-master/dist/wow.min.js"></script>
    <script src="~/Content/Assets/Front/js/theme.js"></script>
</body>
</html>